<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.r.Animal" table="ANIMAL">
		<id name="id" type="int">
			<column name="ID" />
			<generator class="native" />
		</id>
		<property name="nom"  type="java.lang.String">
			<column name="NOM" />
		</property>
		<property name="espece" type="java.lang.String">
			<column name="ESPECE" />
		</property>
		<property name="age" type="int">
			<column name="AGE" />
		</property>
	</class>
</hibernate-mapping>